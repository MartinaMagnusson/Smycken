@model List<DAL.HelperClasses.AncleJewelryHelper>

@{
    ViewBag.Title = "AncleJewelry";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row margin">
    @foreach (var item in Model)
    {
        <h3>@item.Name</h3>
        <img src="~/Images/@item.ImgURL" alt="@item.ImageName" />
        <p>@item.Description</p>
        <p>@item.Price</p>
    }
</div>